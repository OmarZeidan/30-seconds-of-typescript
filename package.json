{
  "name": "30-seconds-of-typescript",
  "description": "This repo is highly inspired by 30-seconds-of-code[Typescript version].",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "GIT_USER=deepakshrma USE_SSH=true docusaurus deploy",
    "prettify": "prettier *.ts --write && prettier *.js --write && prettier docs/*.md --write"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.58",
    "@docusaurus/preset-classic": "^2.0.0-alpha.58",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "deno bundle util.ts > util.js && yes|cp util.js static/util.js && git add util.js static/util.js && lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|html|css|md|mdx)": [
      "prettier --write"
    ],
    "*.(ts)": [
      "deno fmt"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  }
}