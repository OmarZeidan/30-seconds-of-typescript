{
  "name": "30-seconds-of-typescript",
  "description": "User utility code snippets like lodash for typescript, typescriptreact(tsx) and deno [inspired by 30-seconds-of-code].",
  "version": "0.0.4",
  "private": false,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "code-gen": "node scripts/code_snippet.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "GIT_USER=deepakshrma USE_SSH=true docusaurus deploy",
    "extension": "node scripts/vs_code.js",
    "extension:publish": "node scripts/vs_code.js --publish",
    "prettify": "prettier *.ts --write && prettier *.js --write && prettier docs/*.md --write"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.58",
    "@docusaurus/preset-classic": "^2.0.0-alpha.58",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "deno bundle util.ts > util.js && yes|cp util.js static/util.js && npm run code-gen && git add util.js static/util.js && lint-staged"
    }
  },
  "lint-staged": {
    "*.(html|css|md|mdx)": [
      "prettier --write"
    ],
    "*.(ts|js)": [
      "deno fmt"
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.34.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "vsce": "^1.77.0"
  },
  "main": "util.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepakshrma/30-seconds-of-typescript.git"
  },
  "keywords": [
    "deno",
    "typescript",
    "javascript",
    "productivity",
    "utility",
    "tsx"
  ],
  "author": "deepakshrma <deepak.m.shrma@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deepakshrma/30-seconds-of-typescript/issues"
  },
  "homepage": "https://github.com/deepakshrma/30-seconds-of-typescript#readme",
  "icon": "assets/icon_128x128.png",
  "displayName": "30 seconds of typescript and deno",
  "publisher": "imdeepak",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.json"
      }
    ]
  }
}